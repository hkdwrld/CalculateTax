@model IEnumerable<AuthUser>

@{
    ViewData["Title"] = "UserList";
}

<div class="row d-flex allign-items-center justify-content-center">
    <table class="table">
        <thead>
            <tr>
                <th scope="col">#</th>
                <th scope="col">Username</th>
                <th scope="col">Email</th>
            </tr>
        </thead>
        <tbody>
            @{
                int i = 1;
            }
            @foreach (var item in Model)
            {
                <tr>
                    <th scope="row"> @i </th>
                    <td>@item.UserName</td>
                    <td>@item.Email</td>
                </tr>
                i++;
            }
        </tbody>
    </table>
</div>

@section Scripts {
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
}